    "jsdom": "^27.0.0",
    "micromatch": "^4.0.8",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "to-regex-range": "^5.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^5.4.19",
