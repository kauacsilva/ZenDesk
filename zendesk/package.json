    "electron-builder": "^25.1.8"
  },
  "main": "electron/main.js",
  "author": {
    "name": "Equipe ZenTicket",
    "email": "contato@zenticket.com"
  },
  "build": {
    "appId": "com.zenticket.desktop",
    "productName": "ZenTicket",
    "directories": {
      "buildResources": "build",
      "output": "electron-build"
    },
    "files": [
      "electron-app/dist/**/*",
      "electron/**/*",
      "!**/*.map"
    ],
    "asar": true,
    "artifactName": "ZenTicket-${version}-${os}-${arch}.${ext}",
    "extraMetadata": {
      "main": "electron/main.js"
    },
    "extraResources": [
      {
        "from": "public/icons/icon.png",
        "to": "icon.png"
      }
    ],
    "win": {
      "icon": "public/icons/icon.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}