{
  "name": "vite_react_shadcn_ts",
  "description": "ZenTicket - Plataforma de Suporte Inteligente",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:api:dev\" \"vite\"",
    "dev:web": "vite",
    "build": "vite build",
    "api:dev": "cross-env ASPNETCORE_ENVIRONMENT=Development dotnet run --project TicketSystem.API/TicketSystem.API --no-launch-profile --urls \"http://0.0.0.0:5140\"",
    "api:watch": "cross-env ASPNETCORE_ENVIRONMENT=Development dotnet watch --project TicketSystem.API/TicketSystem.API run --no-launch-profile --urls \"http://0.0.0.0:5140\"",
    "adb:reverse": "adb reverse tcp:5140 tcp:5140",
    "adb:reverse:clear": "adb reverse --remove-all",
    "android:build": "npm run build:android && npx cap sync android",
    "android:apk": "npm run android:build && cd android && gradlew.bat assembleDebug",
    "android:build:release": "npm run android:build && cd android && gradlew.bat assembleRelease",
    "android:bundle": "npm run android:build && cd android && gradlew.bat bundleRelease",
    "android:install": "adb install -r android\\app\\build\\outputs\\apk\\debug\\app-debug.apk",
    "android:run": "npm run android:apk && npm run android:install",
    "android:dev": "concurrently -k \"npm:api:dev\" \"npm:adb:reverse\" \"npm run android:run\"",
    "lint": "eslint .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "preview": "vite preview",
    "build:android": "vite build --mode android",
    "android:sync": "npm run android:build && npx cap sync android",
    "electron:dev": "concurrently \"npm run dev:web\" \"cross-env NODE_ENV=development electron .\"",
    "electron:prebuild": "node ./scripts/prepare-electron-build.mjs",
    "electron:build": "npm run electron:prebuild && npm run build:electron && node ./scripts/run-electron-builder.mjs",
    "build:electron": "vite build --mode electron",
    "api:seed-tickets": "node ./scripts/seed-tickets.mjs"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/status-bar": "^7.0.3",
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
